	color: ;
}

body {
	background-color: #4d0000;
	font-family: Arial;
}

a {
	text-decoration: none;
	color: black;
}

h1, h2, h3{
	margin: 0;
	color: black;
}

#container {
	background-color: white;
	width: 900px;
	margin-left: auto;
	margin-right: auto;
}
#header {
	background-color: #66CCFF;
	color: white;
	text-align: center;
}

#content {
	padding: 20px;
}

#nav {
	width: 180px;
	float: left;
}

#nav ul {
	list-style-type: none;
	padding: 0;
}

#nav .selected
	font-weight: bold;

#main {
	width: 600px;
	float: right;
}

#footer {
	clear: both;
	padding: 10px;
	background-color:#999999;
	color: white;
	text-align: left;
	padding: 10px;
}
